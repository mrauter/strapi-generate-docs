{
  "name": "strapi-generate-docs",
  "version": "1.0.0",
  "description": "Generate API specificaiton",
  "homepage": "http://strapi.io",
  "keywords": [
    "generate",
    "generator",
    "strapi",
    "swagger",
    "oai"
  ],
  "main": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "fs-extra": "~0.26.2",
    "lodash": "~3.10.1"
  },
  "devDependencies": {
    "pre-commit": "~1.1.2",
    "xo": "~0.11.2"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "envs": [
      "node",
      "mocha"
    ],
    "globals": [
      "strapi",
      "angular",
      "$",
      "_"
    ],
    "ignores": [
      "node_modules/**",
      "bower_components/**",
      "coverage/**",
      "test/**"
    ],
    "rules": {
      "babel/object-shorthand": 0,
      "consistent-return": 0,
      "consistent-this": 0,
      "dot-notation": 0,
      "global-require": 0,
      "handle-callback-err": 0,
      "max-nested-callbacks": [
        1,
        6
      ],
      "no-else-return": 0,
      "no-extra-parens": 0,
      "no-implicit-coercion": 0,
      "no-invalid-this": 0,
      "no-throw-literal": 0,
      "no-undef": 0,
      "no-unused-expressions": 0,
      "padded-blocks": 0,
      "prefer-arrow-callback": 0,
      "prefer-reflect": 0,
      "prefer-template": 0,
      "quote-props": 0,
      "strict": 0
    }
  },
  "scripts": {
    "test": "xo"
  }
}
